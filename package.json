{
  "name": "developer-network",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Blake Yeboah <blakekyeboah@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn start --prefix client",
    "client-install": "yarn install --prefix client",
    "dev": "concurrently -kill-others \"yarn run server\" \"yarn run client\"",
    "test": "cross-env NODE_ENV=testing jest --testTimeout=10000"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3"
  },
  "engines": {
    "node": "12.19.0",
    "yarn": "1.22.5"
  }
}
